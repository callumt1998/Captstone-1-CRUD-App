<h1>{{{title}}}</h1>
<form action="/api/members" method="POST" class="member_form">
<div class="form-group">
    <label for="name">Name</label>
    <input type="text" name="name" class="form-control">
</div>
<div class="form-group">
    <label for="email">Email</label>
    <input type="email" name="email" class="form-control">
</div>
<div class="form-group">
    <label for="gender">Gender</label>
    <input type="text" name="gender" class="form-control">
</div>
<div class="form-group">
    <label for="status">Status</label>
    <input type="text" name="status" class="form-control">
</div>
<input type="submit" value="Add Member" class="btn primary"
</form>
<h4>Members</h4>
<ul class="member_list">
  {{#each members}}
    <li>{{this.name}}: {{this.email}}: {{this.gender}}: {{this.status}}</li>
  {{/each}}
</ul>
<a href="/api/members" class="btn primary">Visit API</a>